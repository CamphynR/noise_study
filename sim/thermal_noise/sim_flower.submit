Requirements = (!regexp("^node31", Machine) && !regexp("^node33", Machine) && !regexp("^node36", Machine) && !regexp("^node38", Machine))
getenv=True
STATION=22
batch_name=thermal_noise_sim_s$(STATION)

executable = /user/rcamphyn/envs/mattak_env/bin/python3
arguments = /user/rcamphyn/noise_study/sim/thermal_noise/generate_thermal_noise_traces.py \
    --station $(STATION) --batch_i $(Process) \
    --config sim/thermal_noise/config_flower.json \
    --name_appendix _flower
log = /user/rcamphyn/noise_study/submit/thermal_noise_$(STATION).log
output = /user/rcamphyn/noise_study/submit/thermal_noise_$(STATION).out
error = /user/rcamphyn/noise_study/submit/thermal_noise_$(STATION).err
request_cpus = 1
request_memory = 4000MB
request_disk = 5GB
should_transfer_files = NO
queue 64
