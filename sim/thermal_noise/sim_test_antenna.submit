getenv=True
STATION=11
INDEX=17
batch_name=thermal_noise_sim_s$(STATION)_antenna_$(INDEX)

executable = /user/rcamphyn/envs/mattak_env/bin/python3
arguments = /user/rcamphyn/noise_study/sim/thermal_noise/generate_thermal_noise_traces.py --station $(STATION) --config sim/thermal_noise/config_test_antenna_response_n$(INDEX).json --batch_i $(Process) --name_appendix antenna_n$(INDEX)
